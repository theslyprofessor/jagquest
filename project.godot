config_version=5

[application]

config/name="JagQuest"
config/description="An educational action RPG exploring Southwestern College's ACDM programs"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameData="*res://Data/game_data.gd"

[display]

; Window size for comfortable viewing
; Playable map area is 816 x 968 world units
; At MIN_CAMERA_ZOOM, entire playable area fits in window
window/size/viewport_width=1200
window/size/viewport_height=900
window/size/resizable=false
window/stretch/mode="canvas_items"
window/stretch/aspect="keep"

